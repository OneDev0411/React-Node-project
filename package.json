  "scripts": {
    "develop": "cross-env NODE_ENV=development TS_NODE_PROJECT='server/tsconfig.json' node -r ts-node/register server/main.ts && npx sequelize-cli db:migrate:undo",
    "practice": "npx sequelize-cli db:migrate",
    "clean": "rm -rf dist-web && rm -rf dist-server",
    "build": "npm run clean && npm run build:manifest && npm run build:web && npm run build:server",
    "build:manifest": "node -r ts-node/register ./.configuration/update-manifest/index.ts",
    "build:web": "NODE_ENV=production node -r ts-node/register ./.configuration/build/index.ts",
    "build:server": "rimraf dist-server && cross-env NODE_ENV=production tsc -p server/tsconfig.production.json",
    "start": "NODE_ENV=production node dist-server/server/main.js",
    "deploy": "gcloud run deploy --source . --project=rechat-bagot apps-de-commission-slip --region us-central1"
  },
